<Window x:Class="WpfIp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfIp"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="3*"/>
        </Grid.RowDefinitions>
        <WrapPanel>
        <TextBox x:Name="textBoxIp" MinWidth="400" FontSize="30" />
            <Button x:Name="buttonIp" Content="Lekérdezés" Click="buttonIp_Click" />
        </WrapPanel>



        <StackPanel x:Name="stackData" Grid.Row="1">
            
        </StackPanel>

    </Grid>
</Window>
